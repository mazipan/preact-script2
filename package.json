{
  "name": "preact-script2",
  "version": "1.0.0",
  "description": "Library for insert <script> tag for Preact, inspired by vue-script2",
  "amdName": "script2",
  "main": "dist/preact-script2.js",
  "umd:main": "dist/preact-script2.umd.js",
  "module": "dist/preact-script2.mjs",
  "source": "preact-script2.js",
  "files": [
    "dist",
    "preact-script2.js"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "preact build --template template.html",
    "dev": "preact watch --template template.html",
    "bundle": "microbundle src/preact-script2.js -o dist/ --sourcemap false --compress false --name script2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mazipan/preact-script2.git"
  },
  "keywords": [
    "preact",
    "script",
    "load-script",
    "preact-component"
  ],
  "author": "Irfan Maulana <github.com/mazipan>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mazipan/preact-script2/issues"
  },
  "homepage": "https://github.com/mazipan/preact-script2#readme",
  "devDependencies": {
    "if-env": "^1.0.0",
    "microbundle": "^0.10.1",
    "preact-cli": "^2.0.0"
  },
  "dependencies": {
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0"
  }
}
